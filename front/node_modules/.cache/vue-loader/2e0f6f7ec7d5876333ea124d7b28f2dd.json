ent>\n            <naver-maps\n              :height=\"height\"\n              :center.sync=\"center\"\n              :mapOptions=\"mapOptions\"\n              :initLayers=\"initLayers\"\n              @load=\"onLoad\"\n            >\n              <!-- <naver-marker\n                v-for=\"mas in this.mask\"\n                :key=\"mas.id\"\n                :lat=\"mas.lat\"\n                :lng=\"mas.lng\"\n                @click=\"onMarkerClicked\"\n                @load=\"onMarkerLoaded\"\n              ></naver-marker> -->\n            </naver-maps>\n          </md-card-content>\n        </md-card>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapMutations } from \"vuex\";\nexport default {\n  created() {\n    this.$socket.on(\"storeSales\", data => {\n      this.mask = data;\n      console.log(\"this\", data);\n      //this.setMaskMap(data);\n      // this.mask = [\n      //   {\n      //     addr: \"서울특별시 강동구 올림픽로 777 102호 (암사동)\",\n      //     code: \"11805846\",\n      //     created_at: \"2020/03/16 16:50:00\",\n      //     lat: 37.5505172,\n      //     lng: 127.1274244,\n      //     name: \"암사현대약국\",\n      //     remain_stat: \"empty\",\n      //     stock_at: \"2020/03/16 13:36:00\",\n      //     type: \"01\"\n      //   },\n      //   {\n      //     addr: \"인천광역시 계양구 주부토로 479 (계산동)\",\n      //     code: \"31871160\",\n      //     created_at: \"2020/03/16 16:50:00\",\n      //     lat: 37.5369946,\n      //     lng: 126.7277364,\n      //     name: \"진약국\",\n      //     remain_stat: \"empty\",\n      //     stock_at: \"2020/03/16 10:31:00\",\n      //     type: \"01\"\n      //   }\n      // ];\n    });\n  },\n  computed: {\n    MaskMap() {\n      return this.mask;\n    }\n  },\n  data() {\n    return {\n      center: 22,\n      mask: \"\",\n      width: 800,\n      height: 800,\n      info: false,\n      marker: null,\n      count: 1,\n      map: null,\n      isCTT: false,\n      mapOptions: {\n        lat: 37,\n        lng: 127,\n        minZoom: 15,\n        maxZoom: 18,\n        zoomControl: false,\n        zoomControlOptions: { position: \"TOP_RIGHT\" },\n        mapTypeControl: false\n      },\n      initLayers: [\n        \"BACKGROUND\",\n        \"BACKGROUND_DETAIL\",\n        \"POI_KOREAN\",\n        \"TRANSIT\",\n        \"ENGLISH\",\n        \"CHINESE\",\n        \"KOREAN\"\n      ]\n    };\n  },\n  methods: {\n    ...mapMutations([\"setMaskMap\"]),\n    onLoad(vue) {\n      this.map = vue;\n      this.center = this.map.getCenter();\n    },\n    onWindowLoad(that) {},\n    onMarkerClicked(event) {\n      this.info = !this.info;\n    },\n    onMarkerLoaded(vue) {\n      this.marker = vue.marker;\n    },\n    test() {\n      console.log(\"center\");\n      //this.center = this.map.getCenter();\n    }\n  },\n  mounted() {\n    setInterval(() => this.count++, 1000);\n  }\n};\n</script>\n<style scoped>\n.info-window-container {\n  padding: 10px;\n  width: 300px;\n  height: 100px;\n}\n</style>\n"]}]}